{
    "extends": "./tools/config/seed.tslint.json",
    "rules": {
        "no-empty": false,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-whitespace"
        ],
        "no-trailing-whitespace": false,

        // Enable after fix: https://github.com/mgechev/codelyzer/issues/191
        "no-access-missing-member": false
    }
}
